<p id="notice"><%= notice %></p>

<h1>Jios</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Contact number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jios.each do |jio| %>
      <tr>
        <td><%= jio.name %></td>
        <td><%= jio.contact_number %></td>
        <td><%= link_to 'Show', jio %></td>
        <td><%= link_to 'Edit', edit_jio_path(jio) %></td>
        <td><%= link_to 'Destroy', jio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jio', new_jio_path %>
